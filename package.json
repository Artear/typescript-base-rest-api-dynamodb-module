    "debug",
    "intellisense",
    "compile",
    "transpile",
    "REST",
    "api"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test",
      "examples"
    ],
    "require": [
      "ts-node/register"
    ]
  },
