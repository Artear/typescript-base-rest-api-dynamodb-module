    "mockserver": "npm run build && node build/test/mocks/serverMock.js",
    "preapidoc": "rm -rf docs/",
    "apidoc": "./node_modules/apidoc/bin/apidoc -i src/ -o docs/",
    "prerun-basic-example": "npm run build",
    "run-basic-example": "node build/examples/basic/index.js"
  },
  "keywords": [
    "typescript",
    "example",
    "boilerplate",
    "project",
    "skeleton",
    "debug",
    "intellisense",
    "compile",
    "transpile"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test",
      "examples"
    ],
    "require": [
      "ts-node/register"
    ]
  },
