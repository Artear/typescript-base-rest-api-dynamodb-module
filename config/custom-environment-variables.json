{
  "port": "PORT",
  "aws": {
    "accessKeyId": "AWS_ACCESS_KEY_ID",
    "secretAccessKey": "AWS_SECRET_ACCESS_KEY",
    "region": "AWS_REGION",
    "dynamodb": {
      "endpoint": "DYNAMODB_ENDPOINT",
      "timeout": "DYNAMODB_TIMEOUT",
      "maxRetries": "DYNAMODB_MAXRETRIES",
      "tableName": "DYNAMODB_TABLE_NAME",
      "keyName": "DYNAMODB_KEYNAME"
    },
    "cloudwatch": {
      "logGroupName": "CLOUDWATCH_LOG_GROUP_NAME",
      "logStreamName": "CLOUDWATCH_LOG_STREAM_NAME"
    }

  },
  "elastic": {
    "host": "ELASTIC_HOST",
    "index": "ELASTIC_INDEX",
    "type": "ELASTIC_TYPE"
  },
  "security": {
    "enable": "SECURITY_ENABLE",
    "token": "SECURITY_TOKEN"
  },
  "newrelic": {
    "enable": "NEWRELIC_ENABLE",
    "license_key": "NEWRELIC_LICENSE_KEY",
    "app_name": "NEWRELIC_APP_NAME"
  },
  "morgan": {
    "format": "MORGAN_FORMAT"
  },
  "bugsnag": {
    "enable": "BUGSNAG_ENABLE",
    "api_key": "BUGSNAG_API_KEY",
    "release_stage": "BUGSNAG_RELEASE_STAGE"
  }
}
