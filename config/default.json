{
  "port": 8282,
  "server": {
    "options": {
      "name": "Typescript rest API",
      "version": "0.0.1",
      "timeout": 5000,
      "defaultApiVersion": "1.0.0"
    }
  },
  "aws": {
    "accessKeyId": "",
    "secretAccessKey": "",
    "region": "us-west-2",
    "dynamodb": {
      "endpoint": "http://localhost:8000",
      "timeout": 4000,
      "maxRetries": 2,
      "tableName": "",
      "keyName": "itemId"
    }
  },
  "elastic": {
    "host": "localhost:9200",
    "index": "",
    "type": ""
  },
  "security": {
    "enable": false,
    "token": ""
  },
  "newrelic": {
    "enable": false,
    "license_key": false,
    "app_name": "typescript-base-rest-api"
  },
  "bugsnag": {
    "enable": false,
    "api_key": "",
    "release_stage": "dev"
  },
  "morgan": {
    "format": "combined"
  }
}
